<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.wan.dataM.admin.modular.system.dao.RolesinambarimanagerDao">
    <sql id="Base_Column_List">
        id, dataM_account_id, dataM_account, ambari_user_id, ambari_user_name
    </sql>

    <select id="list" resultType="map">
        select * from rolesinambarimanager
        where 0 = 0
        <if test="condition != null and condition != ''">
            AND dataM_account like CONCAT('%',#{condition},'%')
        </if>
        order by id ASC
    </select>

</mapper>